{
  "name": "taro-cloud-music",
  "version": "1.0.0",
  "private": true,
  "description": "云音乐",
  "author": "peijunlei",
  "scripts": {
    "build:alipay": "taro build --type alipay",
    "build:h5": "taro build --type h5",
    "build:jd": "taro build --type jd",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "build:rn": "taro build --type rn",
    "build:swan": "taro build --type swan",
    "build:tt": "taro build --type tt",
    "build:weapp": "taro build --type weapp",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:weapp": "npm run build:weapp -- --watch",
    "prepare": "husky install",
    "start:h5": "npm run build:h5 -- --watch",
    "start:weapp": "npm run build:weapp -- --watch"
  },
  "lint-staged": {
    "*.{css,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@taroify/core": "^0.0.24-alpha.1",
    "@tarojs/cli": "3.4.0",
    "@tarojs/components": "3.4.0",
    "@tarojs/plugin-framework-react": "3.4.0",
    "@tarojs/react": "3.4.0",
    "@tarojs/runtime": "3.4.0",
    "@tarojs/taro": "3.4.0",
    "ahooks": "^3.1.7",
    "classnames": "^2.3.1",
    "default-passive-events": "^2.0.0",
    "mobx": "^6.3.12",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "taro-hooks": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@tarojs/mini-runner": "3.4.0",
    "@tarojs/webpack-runner": "3.4.0",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.4.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "sort-package-json": "^1.55.0",
    "stylelint": "9.3.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "templateInfo": {
    "name": "taro-hooks",
    "typescript": true,
    "css": "less"
  }
}
